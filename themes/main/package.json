{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "sass:build": "sass --no-source-map bootstrap:css",
        "sass:watch": "sass --no-source-map --watch scss:css"
    },
    "devDependencies": {
        "laravel-mix": "^6.0.39",
        "sass": "^1.69.4",
        "sass-loader": "^12.1.0"
    },
    "dependencies": {
        "bootstrap": "^5.3.0",
        "bootstrap-icons": "^1.10",
        "codemirror": "^5.64.0",
        "imageviewer": "^1.1.0",
        "jquery": "^3.6.0",
        "photoswipe": "^5.2.4",
        "photoswipe-dynamic-caption-plugin": "^1.1.1",
        "slick-carousel": "^1.8.1"
    }
}
